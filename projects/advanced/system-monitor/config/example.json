{
  "monitor": {
    "interval": 5000,
    "maxHistorySize": 1000
  },
  "thresholds": {
    "cpu": {
      "warning": 70,
      "critical": 90
    },
    "memory": {
      "warning": 80,
      "critical": 95
    },
    "disk": {
      "warning": 85,
      "critical": 95
    }
  },
  "webhooks": [
  ],
  "cooldownPeriod": 300000,
  "server": {
    "port": 3000,
    "enableAuth": true
  }
}
